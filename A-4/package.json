{
  "name": "pedidos-online-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "setup": "node scripts/setup.js",
    "populate-data": "node -e \"const { populateSampleData } = require('./utils/sampleData.js'); const { productosService } = require('./services/firebaseService.js'); populateSampleData(productosService);\"",
    "test-stock": "node -e \"const { ejecutarPruebasStock } = require('./utils/stockTest.js'); ejecutarPruebasStock();\"",
    "debug-pedidos": "node utils/debugPedidos.cjs"
  },
  "dependencies": {
    "expo": "~49.0.15",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
